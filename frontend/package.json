{
  "name": "shopify-ai-seo-20-frontend",
  "version": "2.0.0",
  "private": true,
  "description": "AI-powered SEO optimization app for Shopify",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run frontend:dev\"",
    "start": "react-scripts start",
    "server:dev": "nodemon server.js",
    "frontend:dev": "cd frontend && npm start",
    "build": "react-scripts build",
    "build:frontend": "cd frontend && npm run build && npm run copy:build",
    "copy:build": "cp -r frontend/build/* public/",
    "test": "react-scripts test",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "deploy": "npm run build && railway deploy",
    "setup": "npm install && npm run build",
    "clean": "rm -rf node_modules frontend/node_modules frontend/build public/*",
    "docker:build": "docker build -t shopify-ai-seo-20 .",
    "docker:run": "docker run -p 3000:3000 shopify-ai-seo-20",
    "eject": "react-scripts eject"
  },
  "keywords": [
    "shopify",
    "seo",
    "ai",
    "optimization",
    "ecommerce",
    "shopify-app"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/shopify-ai-seo-20.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "@shopify/shopify-api": "^9.0.0",
    "@shopify/admin-api-client": "^0.2.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "node-cron": "^3.0.2",
    "axios": "^1.5.0",
    "multer": "1.4.4",
    "csv-parser": "^3.0.0",
    "express-rate-limit": "^6.8.1",
    "express-validator": "^7.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.17.3",
    "connect-mongo": "^5.0.0",
    "nodemailer": "^6.9.4",
    "joi": "^17.9.2",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0",
    "moment": "^2.29.4",
    "crypto": "^1.0.1",
    "path": "^0.12.7",
    "fs-extra": "^11.1.1",
    "archiver": "^5.3.1",
    "csv-writer": "^1.6.0",
    "excel4node": "^1.8.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "@shopify/polaris": "^10.49.1",
    "@shopify/polaris-icons": "^6.11.3",
    "@shopify/app-bridge": "^3.7.9",
    "@shopify/app-bridge-react": "^3.7.9",
    "@shopify/app-bridge-utils": "^3.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "eslint": "^8.47.0",
    "@types/node": "^20.5.0",
    "prettier": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "web/**/*.js",
      "!web/node_modules/**",
      "!web/coverage/**"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}